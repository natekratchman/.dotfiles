{
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(ber:*)",
      "Bash(brew list:*)",
      "Bash(cat:*)",
      "Bash(claude code config:*)",
      "Bash(echo:*)",
      "Bash(find:*)",
      "Bash(git log:*)",
      "Bash(git show:*)",
      "Bash(git worktree:*)",
      "Bash(head:*)",
      "Bash(nvm ls:*)",
      "Bash(rbenv which:*)",
      "Bash(ruby --version:*)",
      "Bash(spring-fix)",
      "Bash(wc:*)",
      "Read(//private/tmp/**)",
      "Read(/Users/nate.kratchman/.claude/**/*)",
      "Search(/Users/nate.kratchman/.claude/**/*)",
      "WebSearch",
      "mcp__Sentry__find_organizations",
      "mcp__Sentry__get_issue_details",
      "mcp__chrome-devtools__click",
      "mcp__chrome-devtools__fill_form",
      "mcp__chrome-devtools__list_pages",
      "mcp__chrome-devtools__navigate_page",
      "mcp__chrome-devtools__take_screenshot",
      "mcp__chrome-devtools__take_snapshot",
      "mcp__serena__check_onboarding_performed",
      "mcp__serena__find_file",
      "mcp__serena__find_symbol",
      "mcp__serena__get_symbols_overview",
      "mcp__serena__initial_instructions",
      "mcp__serena__list_dir",
      "mcp__serena__list_memories",
      "mcp__serena__onboarding",
      "mcp__serena__read_memory",
      "mcp__serena__search_for_pattern",
      "mcp__serena__think_about_collected_information",
      "mcp__serena__write_memory",
      "mcp__playwright__browser_navigate",
      "mcp__playwright__browser_press_key",
      "mcp__playwright__browser_click",
      "mcp__playwright__browser_type",
      "mcp__playwright__browser_wait_for",
      "mcp__playwright__browser_console_messages",
      "mcp__atlassian__getJiraIssue",
      "Bash(grep:*)",
      "mcp__playwright__browser_take_screenshot",
      "mcp__Sentry__search_events"
    ],
    "deny": [
      "WebFetch",
      "Bash(curl:*)",
      "Read(./.env)",
      "Read(./secrets/**)",
      "mcp__serena__insert_before_symbol",
      "mcp__serena__replace_symbol_body",
      "mcp__serena__insert_after_symbol"
    ],
    "ask": [
      "Bash(git push:*)"
    ],
    "defaultMode": "default"
  },
  "model": "opus",
  "hooks": {
    "PermissionRequest": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "say 'Hey Nate, Claude needs your input'"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Write|Edit|MultiEdit",
        "hooks": [
          {
            "type": "command",
            "command": "bash ~/.claude/scripts/run-prettier.sh"
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "say 'Hey Nate, Claude is done'"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "bash ~/.claude/scripts/statusline-command.sh"
  },
  "outputStyle": "Explanatory",
  "alwaysThinkingEnabled": true
}
