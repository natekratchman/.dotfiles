{
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(ber:*)",
      "Bash(brew cat:*)",
      "Bash(brew formula:*)",
      "Bash(brew info:*)",
      "Bash(brew list:*)",
      "Bash(brew outdated:*)",
      "Bash(brew uses:*)",
      "Bash(cat:*)",
      "Bash(claude code config:*)",
      "Bash(echo:*)",
      "Bash(find:*)",
      "Bash(git log:*)",
      "Bash(git show:*)",
      "Bash(git worktree:*)",
      "Bash(grep:*)",
      "Bash(head:*)",
      "Bash(nvm ls:*)",
      "Bash(rbenv which:*)",
      "Bash(ruby --version:*)",
      "Bash(spring-fix)",
      "Bash(tree:*)",
      "Bash(wc:*)",
      "Read(//private/tmp/**)",
      "Read(/Users/nate.kratchman/.claude/**/*)",
      "Read(/Users/nate.kratchman/code/maji/**/*)",
      "Search(/Users/nate.kratchman/.claude/**/*)",
      "Search(/Users/nate.kratchman/code/maji/**/*)",
      "Skill(*)",
      "WebSearch",
      "mcp__chrome-devtools__*",
      "mcp__context7__get-library-docs",
      "mcp__context7__resolve-library-id",
      "mcp__playwright__browser_*",
      "mcp__serena__check_onboarding_performed",
      "mcp__serena__find_file",
      "mcp__serena__find_symbol",
      "mcp__serena__get_symbols_overview",
      "mcp__serena__initial_instructions",
      "mcp__serena__list_dir",
      "mcp__serena__list_memories",
      "mcp__serena__onboarding",
      "mcp__serena__read_memory",
      "mcp__serena__search_for_pattern",
      "mcp__serena__think_about_collected_information",
      "mcp__serena__write_memory"
    ],
    "deny": ["Read(./.env)", "Read(./secrets/**)"],
    "ask": [
      "Bash(curl:*)",
      "Bash(git push:*)",
      "mcp__serena__insert_before_symbol",
      "mcp__serena__replace_symbol_body",
      "mcp__serena__insert_after_symbol",
      "WebFetch"
    ],
    "defaultMode": "default"
  },
  "hooks": {
    "PermissionRequest": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "say 'Hey Nate, Claude needs your input'"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Write|Edit|MultiEdit",
        "hooks": [
          {
            "type": "command",
            "command": "bash ~/.claude/scripts/run-prettier.sh"
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "say 'Hey Nate, Claude is done'"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "bash ~/.claude/scripts/statusline-command.sh"
  },
  "enabledPlugins": {
    "compound-engineering@every-marketplace": true,
    "document-skills@anthropic-agent-skills": true,
    "example-skills@anthropic-agent-skills": true
  },
  "outputStyle": "Explanatory",
  "syntaxHighlightingDisabled": false,
  "alwaysThinkingEnabled": true
}
